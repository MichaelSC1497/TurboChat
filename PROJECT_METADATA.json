{
  "name": "TurboChat",
  "version": "1.0.0",
  "description": "Advanced AI Chat Application with RAG, Web Search, and Quiz System",
  "author": {
    "name": "Nazir YOUSSOUF YAYE",
    "github": "WKingston17",
    "url": "https://github.com/WKingston17"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/WKingston17/TurboChat.git"
  },
  "license": "MIT",
  "technologies": {
    "backend": {
      "language": "Python",
      "framework": "FastAPI",
      "ai_framework": "LangChain",
      "vector_db": "ChromaDB",
      "local_llm": "llama.cpp"
    },
    "frontend": {
      "language": "JavaScript",
      "framework": "React",
      "build_tool": "Vite",
      "ui_library": "Mantine"
    }
  },
  "features": [
    "Multi-model AI chat (OpenAI, Gemini, Groq, OpenRouter)",
    "Local LLM support with llama.cpp",
    "RAG (Retrieval-Augmented Generation) system",
    "Real-time web search integration",
    "Educational quiz system",
    "Document indexing and search",
    "Conversation management",
    "Token usage tracking",
    "Streaming responses"
  ],
  "supported_apis": [
    "OpenAI",
    "Google Gemini",
    "Groq",
    "OpenRouter",
    "SerpAPI"
  ],
  "file_types_supported": [
    "PDF",
    "DOCX",
    "TXT",
    "MD"
  ],
  "languages": [
    "French",
    "English"
  ],
  "keywords": [
    "ai",
    "chatbot",
    "rag",
    "llm",
    "fastapi",
    "react",
    "langchain",
    "vector-database",
    "document-search",
    "quiz-system"
  ]
} 